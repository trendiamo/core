#!/bin/sh

yarn fetch-improv
gzip -9 -c bin/tmp/improv-data.json | aws s3 cp - s3://improv/improv-data.json --profile do --endpoint-url https://improv.ams3.digitaloceanspaces.com/ --content-type application/json --content-encoding gzip --acl public-read --cache-control 'public,max-age=600' --region eu-central-1
rm bin/tmp/improv-data.json
