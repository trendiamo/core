<div data-section-id="{{ section.id }}" data-section-type="hero-section" style="overflow:hidden;" >
  {%- if section.settings.hero_layout == 'fixed_width' -%}
    <div class="page-width">
  {%- endif -%}

    <div class="hero-images-container" data-layout="{{ section.settings.hero_layout }}" >
      <div class="hero-images" >
        <div class="hero-image-container" >
          <img src="{{ section.settings.image_1 | img_url: '900x1350' }}" alt="">
        </div>
        <div class="hero-image-container" >
          <img class="hero-main-image" src="{{ section.settings.image_2 | img_url: '900x1350' }}" alt="">
        </div>
        <div class="hero-image-container" >
          <div class="hero-image-half-width" >
            <img src="{{ section.settings.image_3 | img_url: '900x1350' }}" alt="">
          </div>
          <div class="hero-image-half-width" >
            <img src="{{ section.settings.image_4 | img_url: '900x1350' }}" alt="">
          </div>
        </div>
      </div>
      <div class="hero-images-mobile">
        <img class="hero-main-image-mobile" src="{{ section.settings.image_5 | img_url: '900x1350' }}" alt="">
      </div>
      <div class="hero-block" >
        <h3 class="hero-title" >{{ 'homepage.hero.heading' | t }}</h3>
        <div class="hero-description" >
          <p>{{ 'homepage.hero.subheading' | t }}</p>
        </div>
        <br />
        <div class="hero-block-content">
          <div class="hero-description2" >
            <p>{{ 'homepage.hero.subheading2_html' | t }}</p>
          </div>
          <input class="nl-input nl-input-full-width" type="email" value="" placeholder="{{ 'layout.form.your_email_address' | t }}" />
          <button class="btn-signup btn-lg btn-full-width btn-signup-for-free" >
            {{ 'layout.buttons.sign_up_for_free' | t }}
          </button>
          <a href="https://api.uptous.co/shopify_store_api/v1/oauth/facebook" >
            <button class="btn-signup btn-lg btn-full-width btn-signup-with-facebook" >
              <img src="{{ '../assets/icon-facebook-2.svg' | asset_url }}" alt="">
              {{ 'layout.buttons.sign_up_with_facebook' | t }}
            </button>
          </a>
          <a href="https://api.uptous.co/shopify_store_api/v1/oauth/google" >
            <button class="btn-signup btn-lg btn-full-width btn-signup-with-google" >
              <img src="{{ '../assets/icon-google.svg' | asset_url }}" alt="">
              {{ 'layout.buttons.sign_up_with_google' | t }}
            </button>
          </a>
        </div>
        <p class="text-xs text-center" >{{ 'layout.form.clicking_agreement_html' | t: link: pages['terms-and-conditions'].url }}</p>
      </div>
    </div>

  {%- if section.settings.hero_layout == 'fixed_width' -%}
    </div>
  {%- endif -%}
</div>

